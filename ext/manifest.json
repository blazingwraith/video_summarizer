{
  "manifest_version": 3,
  "name": "Youtube Video Summariser",
  "description": "An extension to summarise youtube videos.",
  "version": "1.0",

  "action": {
    "default_title": "Summarise the video",
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_popup": "popup.html"
  },

  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
